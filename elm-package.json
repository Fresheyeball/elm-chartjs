{
    "version": "1.0.0",
    "summary": "Chartjs Bindings for Elm",
    "repository": "https://github.com/Fresheyeball/elm-chartjs.git",
    "license": "BSD3",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Chartjs.Line",
        "Chartjs.Bar",
        "Chartjs.Radar",
        "Chartjs.Polar",
        "Chartjs.Pie",
        "Chartjs.Doughnut"
    ],
    "native-modules": true,
    "dependencies": {
        "elm-lang/core": "3.0.0 <= v < 4.0.0",
        "evancz/elm-html": "4.0.2 <= v < 5.0.0"
    },
    "elm-version": "0.16.0 <= v <= 0.16.0"
}